<div class="postPlace" style="height: 600px;overflow-y: auto; ">
  <div style="display: flex; justify-content: space-around; border-bottom: 1px solid black;">
    <h3>users: <%= users %></h3>
    <h3>posts: <%= posts %></h3>
    <h3>deteted posts: <%= del %></h3>
  </div>
<div class="admin-title">
  <h2>Posts</h2>
</div>

<ul class=" article-ul ">
    <% data.forEach(post => { %>
        <li>
            <a href="/admin/editPost/<%= post._id %>">
                <%= post.title %> &nearr;
                <br>
                <p>Posted by: <%= post.author.username %></p>
                <br>
                <% if (post.status === 'false') { %>
                  <p>Deleted Post</p>
                <% } %>
                
                <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
            </a>
            
        </li>

    <% }) %>
</ul>
</div>